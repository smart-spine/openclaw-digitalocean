{
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "port": 18789,
    "controlUi": {
      "allowedOrigins": [
        "http://127.0.0.1:18789",
        "http://localhost:18789"
      ]
    },
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    }
  },
  "auth": {
    "profiles": {
      "openai:main": {
        "provider": "openai",
        "mode": "api_key"
      }
    },
    "order": {
      "openai": [
        "openai:main"
      ]
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "openai/gpt-5.1-codex"
      }
    }
  },
  "logging": {
    "redactSensitive": "tools"
  }
}
